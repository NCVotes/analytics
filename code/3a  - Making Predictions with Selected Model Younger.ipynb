    "                    'zip_code', \n",
    "                    'birth_state', \n",
    "                    'drivers_lic', \n",
    "                    'race_gender', \n",
    "                    'race_ethnicity', \n",
    "                    'gender_ethnicity',\n",
    "                    'age_squared', \n",
    "                    'age_cubed',\n",
    "                    'years_since_registration',\n",
    "                    'years_squared',\n",
    "                    'years_cubed'\n",
    "                    ] "
   ]
  },
